@page
@model MyApp.Namespace.ChooseInstructorModel
@{
    Layout = "/Pages/Shared/_StudentLayout.cshtml";

}   
<h1>Choose Instructor</h1>

<form method="Post">
    <label for="studentID">Student ID:</label>
    <input type="number" name="studentID" id="studentID" placeholder="0" required />

    <label for="courseID">Course ID:</label>
    <input type="number" name="courseID" id="courseID" placeholder="Enter" required />

    <label for="InstructorID">Instructor ID:</label>
    <input type="number" name="InstructorID" id="InstructorID" placeholder="Enter" required />

    <label for="current_semester_code">Current Semester Code:</label>
    <input type="text" name="current_semester_code" id="current_semester_code" placeholder="Enter" required />




    <button>Choose Instructor</button>
</form>